{
  "name": "redis-lsm-timeseries",
  "version": "0.0.1",
  "description": "LSM on Redis for Timeseries DB",
  "main": "index.js",
  "scripts": {
    "mocha": " nyc --reporter=html --reporter=text mocha",
    "test": "npm run redisstart && npm run mocha && npm run redisstop && npm run redisstart && cross-env REDISCLIENT=1 npm run mocha && npm run redisstop",
    "perf": "npm run redisstart && concurrently \"npm run service\" \"npm run jmeter\"",
    "jmeter": "run-script-os",
    "redisstop": "docker stop streamz",
    "redisstart": "(npm run redisstop || node -v )&& docker run --name streamz -p 6379:6379 -itd --rm redis:latest",
    "rediscli": "docker exec -it streamz redis-cli",
    "jmeter:windows": "docker run --rm --name jmeter -i -v ${PWD}:${PWD} -w ${PWD} justb4/jmeter:latest -n -t ${PWD}/perf/PerfTest.jmx -l ${PWD}/perf/jmeter.jtl -j ${PWD}/perf/jmeter.log -f -e -o ${PWD}/perf/results -JTARGET_HOST=172.17.0.1",
    "jmeter:macos": "docker run --rm --name jmeter -i -v ${PWD}:${PWD} -w ${PWD} justb4/jmeter:latest -n -t ${PWD}/perf/PerfTest.jmx -l ${PWD}/perf/jmeter.jtl -j ${PWD}/perf/jmeter.log -f -e -o ${PWD}/perf/results -JTARGET_HOST=192.168.1.101",
    "jmeter:nix": "docker run --rm --name jmeter -i -v ${PWD}:${PWD} -w ${PWD} justb4/jmeter:latest -n -t ${PWD}/perf/PerfTest.jmx -l ${PWD}/perf/jmeter.jtl -j ${PWD}/perf/jmeter.log -f -e -o ${PWD}/perf/results -JTARGET_HOST=172.17.0.1",
    "service": "node examples/wrap-it-into-microservice/service.js",
    "service-muted": "node examples/wrap-it-into-microservice/service.js Mute",
    "jmeter-gui": "bash /home/chromeboook/apache-jmeter-5.4.1/bin/jmeter"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LRagji/redis-lsm-timeseries.git"
  },
  "keywords": [
    "Timeseries",
    "LSM",
    "Redis",
    "Laukik"
  ],
  "author": "Laukik",
  "license": "SEE LICENSE IN license.md",
  "bugs": {
    "url": "https://github.com/LRagji/redis-lsm-timeseries/issues"
  },
  "homepage": "https://github.com/LRagji/redis-lsm-timeseries#readme",
  "dependencies": {
    "redis-scripto": "^0.1.3",
    "redis-scripto2": "^0.2.2",
    "shortid": "^2.2.16"
  },
  "devDependencies": {
    "compression": "^1.7.4",
    "concurrently": "^6.2.0",
    "cross-env": "^7.0.3",
    "express": "^4.17.1",
    "ioredis": "^4.27.6",
    "mocha": "^9.0.2",
    "nyc": "^15.1.0",
    "redis": "^3.1.2",
    "redis-streams-broker": "0.0.12",
    "run-script-os": "^1.1.6"
  }
}