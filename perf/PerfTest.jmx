            <stringProp name="script">import groovy.json.JsonOutput

Thread thread = Thread.currentThread();
BigInteger latestdate = new BigInteger(vars.get(&quot;Time&quot;)); 
def range = new HashMap&lt;String,Long&gt;();
range.put(&quot;start&quot;, latestdate-2990000); 
range.put(&quot;end&quot;, latestdate+1990000); 


def requestBody= new HashMap&lt;String,HashMap&lt;String,Long&gt;&gt;();
for(int tagcounter = 0;tagcounter&lt;1;tagcounter++) { 
   	def tagName= &quot;Gen&quot;+thread.getId()+&apos;Tag&apos;+tagcounter;
   	requestBody.put(tagName,range); 
}

vars.put(&quot;rb&quot;,JsonOutput.toJson(requestBody));</stringProp>
