            <stringProp name="parameters"></stringProp>
            <stringProp name="script">import groovy.json.JsonOutput

Thread thread = Thread.currentThread();
Date latestdate = new Date(); 
def predefinedSamples = new HashMap&lt;String,Float&gt;();
for(int timecounter = 0;timecounter&lt;10;timecounter++) { 
   	predefinedSamples.put((new BigInteger(vars.get(&quot;Time&quot;))+(timecounter*1000)+&quot;&quot;), timecounter); 
}


def requestBody= new HashMap&lt;String,HashMap&lt;String,Float&gt;&gt;();
for(int tagcounter = 0;tagcounter&lt;200;tagcounter++) { 
   	def tagName= &quot;Gen&quot;+thread.getId()+&apos;Tag&apos;+tagcounter;
   	requestBody.put(tagName,predefinedSamples); 
}

vars.put(&quot;rb&quot;,JsonOutput.toJson(requestBody));</stringProp>
